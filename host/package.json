{
  "name": "remote",
  "private": true,
  "version": "1.0.0",
  "description": "\"container app\"",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "webpack serve --config webpack.dev.config.js ",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "generate": "graphql-codegen --config codegen.yml",
    "serve": "node dist/server/main.js",
    "build:client": "rimraf dist/client && webpack --progress --config config/webpack.client.js",
    "build:server": "rimraf dist/server && webpack --progress --config config/webpack.server.js",
    "build": "npm run build:server && npm run build:client"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/typescript": "^2.8.6",
    "@graphql-codegen/typescript-operations": "^2.5.11",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/js-cookie": "^3.0.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^8.0.0",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "process": "^0.11.10",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@module-federation/node": "^0.10.2",
    "@module-federation/typescript": "^2.1.3",
    "@types/react-helmet": "^6.1.6",
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.6.1"
  }
}
